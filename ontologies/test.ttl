@prefix : <http://ont.enapso.com/test#> .
@prefix wgs: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix gn: <http://www.geonames.org/ontology#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix fn: <http://www.w3.org/2005/xpath-functions#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix path: <http://www.ontotext.com/path#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sfn: <http://www.w3.org/ns/sparql#> .
@prefix sesame: <http://www.openrdf.org/schema/sesame#> .
@prefix rdf4j: <http://rdf4j.org/schema/rdf4j#> .

<http://ont.enapso.com/test> a owl:Ontology;
  rdfs:comment "(C) Copyright 2021-2022 Innotrade GmbH, Herzogenrath, NRW, Germany"@en,
    "ENAPSO Test Ontology"@en;
  owl:versionIRI <http://ont.enapso.com/test/1.0.0> .

:firstName a owl:DatatypeProperty;
  rdfs:subPropertyOf :name .

:name a owl:DatatypeProperty .

:lastName a owl:DatatypeProperty;
  rdfs:subPropertyOf :name .

:Instance a owl:Class .

:Person a owl:Class;
  rdfs:subClassOf _:node2, _:node3 .

:TestClass a owl:Class .

:Person_AlexanderSchulze a :Person, owl:NamedIndividual;
  :firstName "Alexander";
  :lastName "Schulze" .

:Person_AsheshGoplani a :Person, owl:NamedIndividual;
  :firstName "Ashesh";
  :lastName "Goplani" .

:Person_MuhammadYasir a :Person, owl:NamedIndividual;
  :firstName "Muhammad";
  :lastName "Yasir" .

_:node2 a owl:Restriction;
  owl:onProperty :firstName;
  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger;
  owl:onDataRange xsd:string .

_:node3 a owl:Restriction;
  owl:onProperty :lastName;
  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger;
  owl:onDataRange xsd:string .
